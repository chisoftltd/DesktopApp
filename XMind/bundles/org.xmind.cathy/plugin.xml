   <extension
         id="fileMenuContributions"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=fileStart">
         <command
               commandId="org.eclipse.ui.newWizard"
               id="new"
               label="%menu.file.new.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=close.group">
         <command
               commandId="org.eclipse.ui.file.close"
               id="close"
               label="%menu.file.close.label"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.file.closeAll"
               id="closeAll"
               label="%menu.file.closeAll.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=save.group">
         <command
               commandId="org.eclipse.ui.file.save"
               id="save"
               label="%menu.file.save.label"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.file.saveAs"
               id="saveAs"
               label="%menu.file.saveAs.label"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.file.saveAll"
               id="saveAll"
               label="%menu.file.saveall.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=print.group">
         <command
               commandId="org.eclipse.ui.file.print"
               id="print"
               label="%menu.file.print.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=import.group">
         <command
               commandId="org.eclipse.ui.file.import"
               id="import"
               label="%menu.file.import.label"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.file.export"
               id="export"
               label="%menu.file.export.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=share.group">
         <menu
               id="shareMenu"
               label="%menu.file.share.label">
            <separator
                  name="shareStart"
                  visible="false">
            </separator>
            <separator
                  name="group.intranet"
                  visible="false">
            </separator>
            <separator
                  name="group.internet"
                  visible="false">
            </separator>
            <separator
                  name="group.thirdparty"
                  visible="false">
            </separator>
            <separator
                  name="shareEnd"
                  visible="false">
            </separator>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?before=fileEnd">
         <command
               commandId="org.eclipse.ui.file.exit"
               id="quit"
               label="%menu.file.exit.label"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <not>
                  <reference
                        definitionId="org.xmind.ui.platform.isMac">
                  </reference>
               </not>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
