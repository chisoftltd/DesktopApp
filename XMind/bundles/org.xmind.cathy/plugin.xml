         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.ui.file.closePart"
            contextId="org.xmind.ui.contexts.dashboard"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ESC">
      </key>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.xmind.cathy.internal.WindowPropertyTester"
            id="org.xmind.ui.propertyTester.window"
            namespace="org.xmind.ui.window"
            properties="showingDashboard"
            type="org.eclipse.ui.IWorkbenchWindow">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.xmind.ui.expression.showingDashboard">
         <with
               variable="activeWorkbenchWindow">
            <test
                  property="org.xmind.ui.window.showingDashboard"
                  value="true">
            </test>
         </with>
      </definition>
      <definition
            id="org.xmind.ui.platform.isMac">
                  <or>
                     <systemTest
                           property="osgi.ws"
                           value="carbon">
                     </systemTest>
                     <systemTest
                           property="osgi.ws"
                           value="cocoa">
                     </systemTest>
                  </or>
      </definition>
   </extension>
   <extension
         id="toolbarContributions"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.xmind.ui.toolbar.dashboard?after=start">
         <command
               commandId="org.xmind.ui.command.showDashboard"
               disabledIcon="icons/toolbar/d/dashboard.png"
               icon="icons/toolbar/e/dashboard.png"
               id="org.xmind.ui.toolbar.dashboard.showDashboard"
               label="%toolbar.dashboard.home.label"
               mode="FORCE_TEXT"
               style="push"
               tooltip="%toolbar.dashboard.home.tooltip">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.xmind.ui.toolbar.edit?after=start">
         <command
               commandId="org.eclipse.ui.file.save"
               disabledIcon="icons/toolbar/d/save.png"
               icon="icons/toolbar/e/save.png"
               id="org.xmind.ui.toolbar.edit.save"
               label="%toolbar.edit.save.label"
               style="push"
               tooltip="%toolbar.edit.save.tooltip">
         </command>
         <command
               commandId="org.eclipse.ui.edit.undo"
               disabledIcon="icons/toolbar/d/undo.png"
               icon="icons/toolbar/e/undo.png"
               id="org.xmind.ui.toolbar.edit.undo"
               label="%toolbar.edit.undo.label"
               style="push"
               tooltip="%toolbar.edit.undo.tooltip">
         </command>
         <command
               commandId="org.eclipse.ui.edit.redo"
               disabledIcon="icons/toolbar/d/redo.png"
               icon="icons/toolbar/e/redo.png"
               id="org.xmind.ui.toolbar.edit.redo"
               label="%toolbar.edit.redo.label"
               style="push"
               tooltip="%toolbar.edit.redo.tooltip">
         </command>
      </menuContribution>
   </extension>

